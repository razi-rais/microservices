@{
    ViewBag.Title = "French Revolution | Historic Events";
}

<h3>@ViewBag.Message</h3>

@model List<BusinessLayer.HistoricEvent>



<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Date</td>
                <td>@item.Description</td>
            </tr>

        }
    </tbody>
</table>